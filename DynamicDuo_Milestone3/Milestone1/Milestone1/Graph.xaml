<Window
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Milestone1"
        xmlns:chartingToolkit="clr-namespace:System.Windows.Controls.DataVisualization.Charting;assembly=System.Windows.Controls.DataVisualization.Toolkit" x:Class="Milestone1.Graph"
        mc:Ignorable="d"
        Title="Graph" Height="510.92" Width="627.586">
    <Grid>
        <chartingToolkit:Chart HorizontalAlignment="Left" Height="482" VerticalAlignment="Top" Width="621" Title="Check-Ins" Name="MyChart" Margin="0,0,-0.2,-0.4">
            <chartingToolkit:ColumnSeries DependentValuePath="Value" IndependentValuePath="Key" ItemsSource="{Binding}" Title="My Data"/>
        </chartingToolkit:Chart>
    </Grid>
</Window>
